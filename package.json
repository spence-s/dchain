{
  "name": "lassify",
  "version": "0.1.1",
  "description": "cli for lass",
  "keywords": [
    "lass",
    "lassify"
  ],
  "homepage": "https://github.com/lassjs/lassify",
  "bugs": {
    "url": "https://github.com/lassjs/lassify/issues",
    "email": "sasnyde2@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lassjs/lassify.git"
  },
  "license": "MIT",
  "author": {
    "name": "Spencer Snyder",
    "email": "sasnyde2@gmail.com",
    "url": "https://spencersnyder.io"
  },
  "contributors": [
    {
      "name": "Spencer Snyder",
      "email": "sasnyde2@gmail.com",
      "url": "https://spencersnyder.io"
    }
  ],
  "type": "module",
  "main": "src/cli.js",
  "bin": {
    "lassify": "src/cli.js"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "yarn run lint:js && yarn run lint:md && yarn run lint:package",
    "lint:js": "xo",
    "lint:md": "prettier --check **/*.md",
    "lint:package": "npmPkgJsonLint ./package.json && prettier --check --plugin=prettier-plugin-packagejson ./package.json",
    "test": "cross-env NODE_ENV=test ava",
    "test-coverage": "cross-env NODE_ENV=test nyc yarn run test",
    "ug": "ncu -i"
  },
  "ava": {
    "verbose": true
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/prettier": "^2.7.1",
    "cosmiconfig": "^7.0.1",
    "debug": "^4.3.4",
    "execa": "^6.1.0",
    "load-json-file": "^7.0.1",
    "lodash": "^4.17.21",
    "meow": "^10.1.3",
    "npm-check-updates": "^15.3.4",
    "npm-conf": "^1.1.3",
    "npm-package-json-lint": "^6.3.0",
    "ora": "^6.1.2",
    "path-exists": "^5.0.0",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "read-pkg-up": "^9.1.0",
    "semver": "^7.3.7",
    "superb": "^4.0.0",
    "tmp-promise": "^3.0.3",
    "validate-npm-package-name": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/prompts": "^2.4.1",
    "@types/semver": "^7.3.13",
    "@types/validate-npm-package-name": "^4.0.0",
    "ava": "^4.3.1",
    "codecov": "^3.8.2",
    "cross-env": "latest",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-package-json-lint-config-default": "^5.0.0",
    "nyc": "^15.1.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "sinon": "^14.0.0",
    "type-fest": "^3.2.0",
    "typescript": "^4.8.4",
    "xo": "^0.52.4"
  },
  "engines": {
    "node": ">= 16"
  }
}
